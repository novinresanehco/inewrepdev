<head>
<meta name="viewport" content="width=device-width,initial-scale=1">
<title>[sitetitle]</title>
                    <meta name="keywords" content="[sitekeywords]">
                        <meta name="description" content="[sitedescription]">
<meta name="robots" content="index,follow" />
							<base href="[site]" target="_self">
<link rel="canonical" href="[site]" />
<link rel="alternate" type="application/rss+xml" title="title" href="[site]/rss.xml" />
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

<!-- main JS libs -->
<script src="[site]theme/core/fun/js/libs/modernizr.min.js"></script>
<script src="[site]theme/core/fun/js/libs/jquery-1.10.0.js"></script>
<script src="[site]theme/core/fun/js/libs/jquery-ui.min.js"></script>
<script src="[site]theme/core/fun/js/libs/bootstrap.min.js"></script>

<!-- Style CSS -->
<link href="[site]theme/core/fun/css/bootstrap.css" media="screen" rel="stylesheet">
<link href="[site]theme/core/fun/style.css" media="screen" rel="stylesheet">

<!-- scripts -->
<script src="[site]theme/core/fun/js/general.js"></script>
<!-- custom input -->
<script src="[site]theme/core/fun/js/jquery.customInput.js"></script>
<script type="text/javascript" src="[site]theme/core/fun/js/custom.js"></script>
<!-- Placeholders -->
<script type="text/javascript" src="[site]theme/core/fun/js/jquery.powerful-placeholder.min.js"></script>

  <style type="text/css">
body
{
background: [config_theme_fun_bgcolor] url('[config_theme_fun_mainbg]') repeat;
} 
        </style>
<!-- Scroll Bars -->
<script src="[site]theme/core/fun/js/jquery.mousewheel.js"></script>
<script src="[site]theme/core/fun/js/jquery.jscrollpane.min.js"></script>
<script type="text/javascript">
	jQuery(function()
	{
		jQuery('.scrollbar.style2').jScrollPane({
			verticalDragMaxHeight: 30,
			verticalDragMinHeight:30
		});
	});
</script>

<!--[if lt IE 9]><script src="[site]theme/core/fun/js/respond.min.js"></script><![endif]-->
